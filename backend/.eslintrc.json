{
	"env": {
		"browser": true,
		"commonjs": true,
		"es2021": true
	},
	"extends": ["airbnb-base"],
	"parserOptions": {
		"ecmaVersion": "latest"
	},
	"rules": {
		"no-unused-vars": "off",
		"no-console": "off",
		"no-use-before-define": "off",
		"no-underscore-dangle": "off",
		"no-tabs": 0,
		"indent": [0, "tab"],
		"eol-last": "off",
		"function-call-argument-newline": "off",
		"function-paren-newline": "off",
		"allow-indentation-tabs": "off",
		"max-len": "off",
		"no-param-reassign": "off"
	}
}
